  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "generate": "npx plop -- --plopfile generators/plopfile.js",
    "storybook": "start-storybook --no-manager-cache -s ./public -p 6006",
